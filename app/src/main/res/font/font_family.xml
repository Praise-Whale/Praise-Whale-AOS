<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <font
        android:font="@font/noto_sans_bold"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/noto_sans_bold"
        app:fontStyle="normal"
        app:fontWeight="700"
        tools:targetApi="o" />
    <font
        android:font="@font/noto_sans_semi_bold"
        android:fontStyle="normal"
        android:fontWeight="600"
        app:font="@font/noto_sans_semi_bold"
        app:fontStyle="normal"
        app:fontWeight="600"
        tools:targetApi="o" />
    <font
        android:font="@font/noto_sans_medium"
        android:fontStyle="normal"
        android:fontWeight="500"
        app:font="@font/noto_sans_medium"
        app:fontStyle="normal"
        app:fontWeight="500"
        tools:targetApi="o" />
    <font
        android:font="@font/noto_sans_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/noto_sans_regular"
        app:fontStyle="normal"
        app:fontWeight="400"
        tools:targetApi="o" />

    <font
        android:font="@font/gmarket_sans_medium"
        android:fontStyle="normal"
        android:fontWeight="500"
        app:font="@font/gmarket_sans_medium"
        app:fontStyle="normal"
        app:fontWeight="500"
        tools:targetApi="o" />
</font-family>